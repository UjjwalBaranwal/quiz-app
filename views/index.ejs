      <!-- contact us form -->
      <section class="section-cta" id="cta">
        <div class="container">
          <div class="cta-grid">
            <div class="cta-text-box">
              <div class="heading-secondary">Have a Question? Ask Away!</div>
              <p class="cta-text">
                Have a question, suggestion, or just want to say hello? We'd
                love to hear from you! Fill out the form below, and let's start
                a conversation. Whether you're a seasoned quiz creator, an
                educator exploring interactive learning, or a curious learner,
                your input matters. Our team is here to assist you, provide
                support, and ensure you have the best experience with "Quiz
                time". Reach out now, and let's make learning and quizzing even
                more enjoyable together!
              </p>
              <form class="cta-form" name="name" action="/" method="post">
                <div>
                  <label for="name">Full Name</label>
                  <input
                    type="text"
                    placeholder="Your Name"
                    id="name"
                    name="name"
                    required
                  />
                </div>
                <div>
                  <label for="email">Email address</label>
                  <input
                    type="email"
                    placeholder="me@example.com"
                    id="email"
                    name="email"
                    required
                  />
                </div>
                <div>
                  <label for="hearedFrom">Where you here about us</label>
                  <select id="hearedFrom" name="hearedFrom" required>
                    <option value="empty">select one option</option>
                    <option value="friends">friends and Family</option>
                    <option value="youtube">Youtube video</option>
                    <option value="podcast">Podcast</option>
                    <option value="ad">Facebook Ads</option>
                    <option value="other">Others</option>
                  </select>
                </div>
                <button type="submit" class="btn btn--form btn--contact">
                  contact us
                </button>
              </form>
            </div>

            <div
              class="cta-img-box"
              role="img"
              aria-label="women enjoying food"
            ></div>
          </div>
        </div>
      </section>
      <!-- student dashboard section -->
      <section id="studentDashboard" class="studentDashboard">
        <div class="tab-container">
          <div class="wrapper">
            <ul class="indicator">
              <li class="active" data-target="#findQuiz">
                <ion-icon name="search"></ion-icon>Find Quiz
              </li>
              <li data-target="#profile">
                <ion-icon name="person-circle"></ion-icon>Profile
              </li>
              <li data-target="#help"><ion-icon name="call"></ion-icon>Help</li>
            </ul>
            <ul class="tab-content">
              <li class="active" id="findQuiz">
                <div class="tab-heading">
                  <h1>Find Quiz</h1>
                  <div class="user-profile">
                    <div class="action">
                      <div class="profile" onclick="menuToggle();">
                        <img src="img/user.png" alt="" />
                      </div>
                      <div class="menu">
                        <h3>
                          User Name
                          <div>Student</div>
                        </h3>
                        <ul>
                          <li>
                            <ion-icon
                              class="icons-size"
                              name="person-circle"
                            ></ion-icon>
                            <a href="#">My Profile</a>
                          </li>
                          <li>
                            <ion-icon class="icons-size" name="call"></ion-icon>
                            <a href="#">Help</a>
                          </li>
                        </ul>
                        <div class="Log-Out">
                          <ion-icon
                            class="icons-size"
                            class="Log-Out"
                            name="log-out-outline"
                          ></ion-icon>
                          <a href="#">Log Out</a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="fq-container">
                  <p>
                    Access the quiz link and discover a curated selection of
                    quizzes thoughtfully prepared by your teacher. Enter the
                    quiz portal to explore the educational journey your teacher
                    has crafted just for you.
                  </p>
                  <div class="search">
                    <form class="form">
                      <input
                        id="search"
                        type="text"
                        class="input"
                        placeholder="Enter quiz url"
                      />
                      <button id="submit" class="submit-btn">Take Quiz</button>
                    </form>
                  </div>
                </div>
              </li>
              <li id="profile">
                <div class="pf-container">
                  <h1>Profile</h1>
                  <p>
                    Access the quiz link and discover a curated selection of
                    quizzes thoughtfully prepared by your teacher.
                  </p>
                </div>
              </li>
              <li id="help">
                <h1>Help</h1>
                <p>
                  Access the quiz link and discover a curated selection of
                  quizzes thoughtfully prepared by your teacher.
                </p>
              </li>
            </ul>
          </div>
        </div>
